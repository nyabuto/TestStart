        <servlet-class>Loaders.load_counties</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_sub_counties</servlet-name>
        <servlet-class>Loaders.load_sub_counties</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_facilities</servlet-name>
        <servlet-class>Loaders.load_facilities</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_quarter</servlet-name>
        <servlet-class>Loaders.load_quarter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadTXCURR</servlet-name>
        <servlet-class>TestStart.UploadTXCURR</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dbsetup</servlet-name>
        <url-pattern>/dbsetup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadExcels</servlet-name>
        <url-pattern>/UploadExcels</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RawData</servlet-name>
        <url-pattern>/RawData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_years</servlet-name>
        <url-pattern>/load_years</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_months</servlet-name>
        <url-pattern>/load_months</url-pattern>
    </servlet-mapping>
