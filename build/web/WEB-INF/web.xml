    <servlet-mapping>
        <servlet-name>UploadTXCURR</servlet-name>
        <url-pattern>/UploadTXCURR</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
